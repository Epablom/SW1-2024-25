<!DOCTYPE html>
<html lang="en">

<%- include('partials/head') %>

<body>
    <div class="container">
        <!-- Sidebar Section -->
        <%- include('partials/leftBar') %>
        <!-- End of Sidebar Section -->

        <!-- Main Content -->
        <main>
            <h1>Panel de Administración</h1>
            <!-- Analyses -->
            <div class="analyse">
                <div class="sales">
                    <div class="status">
                        <div class="info">
                            <h3>Nuevos apuntes</h3>
                            <h1>24</h1>
                        </div>
                        <div class="progresss">
                            <svg>
                                <circle cx="38" cy="38" r="36"></circle>
                            </svg>
                            <div class="percentage">
                                <p>+81%</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="visits">
                    <div class="status">
                        <div class="info">
                            <h3>Visitas mensuales</h3>
                            <h1>1,181</h1>
                        </div>
                        <div class="progresss">
                            <svg>
                                <circle cx="38" cy="38" r="36"></circle>
                            </svg>
                            <div class="percentage">
                                <p>-48%</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="searches">
                    <div class="status">
                        <div class="info">
                            <h3>Busquedas</h3>
                            <h1>547</h1>
                        </div>
                        <div class="progresss">
                            <svg>
                                <circle cx="38" cy="38" r="36"></circle>
                            </svg>
                            <div class="percentage">
                                <p>+21%</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of Analyses -->

            <!-- New Users Section -->
            <br>
                <h2>Nuevos usuarios</h2>
                <div class="list">
                    <div class="user">
                        <img src="../images/users/1.jpg" alt="User profile image">
                        <h2>Jack</h2>
                        <p>54 Min Ago</p>
                    </div>
                    <div class="user">
                        <img src="../images/users/2.jpg" alt="User profile image">
                        <h2>Paul</h2>
                        <p>3 Hours Ago</p>
                    </div>
                    <div class="user">
                        <img src="../images/users/3.jpg" alt="User profile image">
                        <h2>Ember</h2>
                        <p>6 Hours Ago</p>
                    </div>
                    <div class="user">
                        <img src="../images/users/4.jpg" alt="User profile image">
                        <h2>Isabel</h2>
                        <p>1 Day Ago</p>
                    </div>
                </div>
            <!-- End of New Users Section -->

            <!-- Recent Orders Table -->
            <br>
                <h2>Últimas alertas</h2>
                <div class="list">
                <table>
                    <thead>
                        <tr>
                            <th>Nombre de usuario</th>
                            <th>Publicación reportada</th>
                            <th>Número de tiquet</th>
                            <th>Estado</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
                <a href="#">Ver más</a>
            </div>
            <!-- End of Recent Orders -->

        </main>
        <!-- End of Main Content -->

        <!-- Right Section -->
        <%- include('partials/rightBar') %>
        <!-- End of right section -->


    </div>

    <script src="../public/js/alertas.js"></script>
    <script src="../public/js/index.js"></script>
    
</body>
</html>